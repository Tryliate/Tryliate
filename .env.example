# ============================================================================
# Tryliate v1.1.1 - Neural Intelligence Configuration Template
# ============================================================================
# Copy this file to .env.local and fill in your actual values
# DO NOT commit .env.local to version control
# ============================================================================

# ============================================================================
# SUPABASE (Admin Instance - Required)
# ============================================================================
# This is your central Supabase instance that manages user data and BYOI
# Get these from: https://app.supabase.com/project/_/settings/api

NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_PUBLISHABLE_KEY=sb_publishable_...
SUPABASE_SECRET_KEY=sb_secret_... (replaces legacy Service Role Key)

# ============================================================================
# BACKEND URLS
# ============================================================================
# Development (local):
# NEXT_PUBLIC_CLOUD_RUN_URL=http://localhost:8080
# NEXT_PUBLIC_ENGINE_URL=http://localhost:3001

# Production (Google Cloud Run):
NEXT_PUBLIC_CLOUD_RUN_URL=https://tryliate-backend-374665986758.us-east1.run.app
NEXT_PUBLIC_ENGINE_URL=https://tryliate-engine-nh767yfnoq-ue.a.run.app

# ============================================================================
# AI INFERENCE (Groq - Required)
# ============================================================================
# Get your API key from: https://console.groq.com/keys
# Used for multi-agent orchestration (Validator + Implementer agents)

GROQ_API_KEY=gsk_...

# ============================================================================
# NEURAL MEMORY (OpenAI - REQUIRED for v1.1.1+)
# ============================================================================
# Get your API key from: https://platform.openai.com
# Used for generating 1536-dimensional semantic embeddings
OPENAI_API_KEY=sk-...

# ============================================================================
# SUPABASE OAUTH (Required for BYOI)
# ============================================================================
# Create OAuth app at: https://supabase.com/dashboard/account/apps
# Redirect URI: https://your-domain.com/auth/callback/supabase

NEXT_PUBLIC_SUPABASE_OAUTH_CLIENT_ID=your-oauth-client-id
SUPABASE_OAUTH_CLIENT_SECRET=your_supbase_oauth_secret_here

GOOGLE_CLIENT_ID=your-google-oauth-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your_google_client_secret

# ============================================================================
# LOGO.DEV (Optional - for brand logos)
# ============================================================================
# Get your API key from: https://logo.dev
# Used for displaying company logos in the UI

NEXT_PUBLIC_LOGO_DEV_PUBLISHABLE_KEY=pk_...
LOGO_DEV_SECRET_KEY=sk_...



# ============================================================================
# REDIS (Optional - for caching)
# ============================================================================
# Get a free instance from: https://upstash.com
# Used for caching MCP registry data

REDIS_URL=rediss://default:password@your-redis.upstash.io:6379

# ============================================================================
# DEVELOPMENT SETTINGS
# ============================================================================
# Disable telemetry for faster builds

NEXT_TELEMETRY_DISABLED=1
TURBO_TELEMETRY_DISABLED=1

# ============================================================================
# NOTES
# ============================================================================
# 1. All NEXT_PUBLIC_* variables are exposed to the browser
# 2. Never commit secrets to version control
# 3. Use Google Secret Manager for production deployments
# 4. Rotate keys regularly for security
# 5. Test locally before deploying to production
# ============================================================================
